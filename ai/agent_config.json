      "auto_issue_creation": true,
      "pr_comments": true
    },
    "slack": {
      "enabled": false,
      "channel": "#ai-alerts"
    },
    "vercel": {
      "enabled": true,
      "metrics_collection": true
    }
  },
  "features": {
    "self_diagnosis": true,
    "insights_agent": true,
    "cost_tracking": true,
    "privacy_guard": true,
    "semantic_search": true,
    "auto_documentation": true
  },
  "privacy": {
    "pii_redaction": true,
    "data_retention_days": 90,
    "telemetry_opt_in": true
  },
  "runtime_targets": [
    "vercel-edge",
    "cloudflare-workers",
    "shopify-oxygen",
    "wasm"
  ]
