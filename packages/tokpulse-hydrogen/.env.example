# Shopify App Configuration
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_API_SECRET=your_api_secret_here
SHOPIFY_SCOPES=read_products,read_orders,read_customers,read_analytics,write_products,write_orders
SHOPIFY_APP_URL=https://your-app-url.com

# Shopify Store Configuration
SHOPIFY_STORE_DOMAIN=your-store.myshopify.com
SHOPIFY_STOREFRONT_TOKEN=your_storefront_token_here
SHOPIFY_STOREFRONT_API_URL=https://your-store.myshopify.com/api/2024-10/graphql.json

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/tokpulse
REDIS_URL=redis://localhost:6379

# Social Media API Keys
INSTAGRAM_API_KEY=your_instagram_api_key
INSTAGRAM_API_SECRET=your_instagram_api_secret
TIKTOK_API_KEY=your_tiktok_api_key
TIKTOK_API_SECRET=your_tiktok_api_secret
FACEBOOK_API_KEY=your_facebook_api_key
FACEBOOK_API_SECRET=your_facebook_api_secret
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret

# App Configuration
NODE_ENV=production
PORT=3000
CORS_ORIGIN=https://your-app-url.com

# Analytics Configuration
ANALYTICS_ENABLED=true
ANALYTICS_SYNC_INTERVAL=3600000
ANALYTICS_DATA_RETENTION=31536000000

# Notification Configuration
EMAIL_SERVICE_API_KEY=your_email_service_api_key
PUSH_NOTIFICATION_KEY=your_push_notification_key
SMS_SERVICE_API_KEY=your_sms_service_api_key

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info